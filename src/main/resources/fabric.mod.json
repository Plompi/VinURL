{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}",
    "${mod_credits}"
  ],
  "contact": {
    "sources": "${mod_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/vinurl/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.vinurl.VinURL"
    ],
    "client": [
      "com.vinurl.client.VinURLClient"
    ]
  },
  "mixins": [
    "vinurl.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${mod_fabric_loader_version}",
    "minecraft": ["${mod_minecraft_version_start}","${mod_minecraft_version_end}"],
    "java": ">=21",
    "fabric-api": "*",
    "owo-lib": "*"
  }
}